vardef borderCut(expr radius, alpha, beta, c) =
	pair s, ss, f, ff;
	s = radius * dir(alpha);
	ss = c * s;
	f = radius * dir(beta);
	ff = c * f;

	s .. ss{dir (180 + alpha)} .. tension 0.75 .. ff{dir beta} .. f
enddef;

vardef borderSlice(expr radius, alpha, beta, c) =
	pair ba, bb;
	ba = radius * dir(a);
	bb = radius * dir(b);

	borderCut(radius, a, b, c) .. bb{dir (b - 90)} .. ba{dir (a - 90)} .. cycle
enddef;


